<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<display-name>love-ejb</display-name>
	<enterprise-beans>
	    <session>
	        <ejb-name>ProductsBean</ejb-name>
	        <security-role-ref>
	            <role-name>admin</role-name>
	        </security-role-ref>
	        <security-identity>
	            <use-caller-identity/>
	        </security-identity>
	    </session>
	</enterprise-beans>
	<assembly-descriptor>
	    <security-role>
	        <role-name>admin</role-name>
	    </security-role>
	</assembly-descriptor>
</ejb-jar>